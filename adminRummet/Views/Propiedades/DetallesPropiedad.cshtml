                    </div>
                    @{
                        // Obtener la fecha actual
                        var fechaActual = DateTime.Now;
                        // Definir la fecha de publicación (puede ser una variable dinámica en tu aplicación)
                        var fechaPublicacion = DateTime.Parse(@Model.FPublicacion);
                        // Calcular la diferencia en días
                        var diasDesdePublicacion = (fechaActual - fechaPublicacion).Days;
                        // Calcular la cantidad de años, meses, semanas y días
                        var años = diasDesdePublicacion / 365;
                        var meses = (diasDesdePublicacion % 365) / 30;
                        var semanas = ((diasDesdePublicacion % 365) % 30) / 7;
                        var dias = ((diasDesdePublicacion % 365) % 30) % 7;
                    }

                    

                    <div class="tiempo-publi  p-1">
                        <h5 class="text-center mx-auto label-blue-2">Tiempo publicada</h5>
