@model PropiedadesModel;


@{
    Layout = "_NavUsuarios";
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Proyecto_administracion</title>

    <link href="~/assets/css/material-bootstrap-wizard.css" rel="stylesheet">
    <link href="~/assets/css/material-bootstrap-wizard-2.css" rel="stylesheet">


    <!--<link rel="stylesheet" href="~/css/style_form_nuevo_usuario.css">
        -->
    <link rel="stylesheet" href="~/css/style_nueva_propiedad.css">
    <link rel="stylesheet" href="~/css/Style_form.css">

    <link rel="icon" type="image/png" sizes="16x16" href="~/img/imagen-logo/logo_oficial.png">
    <script src="https://kit.fontawesome.com/e43b4d35fb.js" crossorigin="anonymous"></script>



</head>


<ul class="circles">
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
</ul>


<div class="area">

    <div class="container-lg ">
        <div class="row">
            <div class="col">
                <!--      Wizard container        -->
                <div class="wizard-container">
                    <div class="card wizard-card glass" data-color="orange" id="wizardProfile">

                        <form asp-controller="Propiedades" asp-action="GuardarPropiedad" enctype="multipart/form-data" method="post">
                            <div class="wizard-navigation ">
                                <ul>
                                    <li><a href="#generales" data-toggle="tab">Generales</a></li>
                                    <li><a href="#direccion" data-toggle="tab">Dirección</a></li>
                                    <li><a href="#superficie" data-toggle="tab">Superficie</a></li>
                                    <li><a href="#caracteristicas" data-toggle="tab">Características</a></li>
                                    <li><a href="#documentos" data-toggle="tab">Documentos</a></li>
                                    <li><a href="#propietario" data-toggle="tab">Propietario</a></li>
                                    <li><a href="#progr_agenda" data-toggle="tab">Programar agenda</a></li>
                                    <li><a href="#vista_previa" data-toggle="tab">Vista previa</a></li>
                                </ul>
                            </div>

                            <div class="tab-content">

                                <!--aqui comienza la primera pestañas GENERALES-->
                                <div class="tab-pane" id="generales">
                                    <div class="row">

                                        <!-- Steps -->
                                        <label for="">
                                            <h5 class="ms-1 color_letter">Datos generales</h5>
                                        </label>
                                        <div class="input-group justify-content-between  d-lg-inline-flex ">
                                            <div class="">
                                                <label class="color_letter2">
                                                    <h6>Tipo de operación</h6>
                                                </label>
                                                <div class="">


                                                    <input asp-for="Operacion" type="radio" value="1" name="Operacion" class="btn-check"
                                                           id="renta" autocomplete="off" checked>

                                                    <label class="btn btn-outline-light rounded-3 bg-white "
                                                           for="renta">Renta</label>

                                                    <input asp-for="Operacion" type="radio" value="2" name="Operacion" class="btn-check"
                                                           id="comprar" autocomplete="off">
                                                    <label class="btn btn-outline-light ms-1 rounded-3 bg-white"
                                                           for="comprar">Venta</label>

                                                </div>


                                            </div>
                                            <div class=" d-lg-inline-block  mb-3  ps-3 width-n-propiedad">

                                                <div class=" d-inline form-group">
                                                    <label for="n_propiedad" class=" color_letter2  control-label ">
                                                        <h6 class="mb-4">
                                                            Nombre de la
                                                            propiedad
                                                        </h6>
                                                    </label>
                                                    <input type="text" asp-for="NombrePropiedad" name="NombrePropiedad" id="nombre_propiedad"
                                                           class="form-control  w-100 margin-t-Npropiedad" />

                                                </div>
                                            </div>

                                            <div class=" d-lg-inline-block col-lg-4  ms-5 ">
                                                <label for="t_propiedad" class="text-start color_letter2">
                                                    <h6>Tipo de propiedad</h6>
                                                </label>

                                                @Html.DropDownListFor(model => model.TPropiedad, new
                                                SelectList(Model.TPropiedadL,"IDtipoP","Tipo"), "Tipo",new
                                                {@class="size-select2 rounded text-center", @id="TipoPropiedadID"})

                                                @Html.DropDownListFor(model => model.SubPropiedad, new List<SelectListItem>(),
                                                "Subtipo", new { @class = "size-select2 rounded text-center d-lg-inline-block mt-md-2", @id="SubTipoPropiedadID" })

                                            </div>
                                        </div>

                                        <hr>


                                        <label for="">
                                            <h5 class="color_letter ms-1">Precio</h5>
                                        </label>

                                        <div class="input-group d-lg-inline-flex justify-content-between
                                                    mt-lg-0 align-content-between row row-cols-2">

                                            <div class="d-flex row row-cols-3 ">

                                                <div class="d-lg-inline-block mt-1 mb-3">
                                                    <label for="" class="text-start color_letter2">
                                                        <h6>Tipo de moneda</h6>
                                                    </label>

                                                    @Html.DropDownListFor(model => model.Moneda, new
                                                    SelectList(Model.MonedaL,"IDMoneda","Moneda"), "Moneda",new
                                                    {@class="size-select2 rounded text-center", @id="IDMoneda"})

                                                </div>
                                                <div class="d-lg-inline-block mt-1 mb-3">
                                                    <div class="d-inline form-group  ">
                                                        <label for="precio" class="control-label color_letter2 ">
                                                            <h6 class="mb-3">Precio</h6>
                                                        </label>
                                                        <input type="number" asp-for="Precio" name="Precio" id="precioProp" class="form-control NobtnNumber"
                                                               min="0" />
                                                    </div>
                                                </div>

                                                <div class="alinear" id="rentaOpcions2">
                                                    <label for="" class="me-3 color_letter2 d-lg-inline-block ">
                                                        <h6 class="color_letter2">Período</h6>
                                                    </label>
                                                    <div id="renta" class="d-block ">

                                                        <label>
                                                            <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="1"
                                                                   class="d-inline-flex border-radio_buttons" />Semanal
                                                        </label>
                                                        <label>
                                                            <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="2"
                                                                   class="d-inline-flex border-radio_buttons" />Mensual

                                                        </label>
                                                        <label>
                                                            <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="3"
                                                                   class="d-inline-flex border-radio_buttons" />Anual
                                                        </label>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="row row-cols-2 alinear" id="rentaOpcions">

                                                <div class="d-flex">
                                                    <div class="form-group mt-0">
                                                        <label for="n_propiedad" class=" control-label color_letter2">
                                                            <h6 class="mb-4">Mantenimiento</h6>
                                                        </label>
                                                        <input asp-for="MantenimientoPrecio" type="text" id="mantenimiento"
                                                               min="0" class="form-control NobtnNumber" />
                                                    </div>
                                                </div>
                                                <div class="d-block" id="renta2">
                                                    <label for="" class="me-3 color_letter2 ">
                                                        <h6 class="color_letter2">Período</h6>
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="2"
                                                               class="d-inline-flex border-radio_buttons" />Semanal
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="1"
                                                               class="d-inline-flex border-radio_buttons" />Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="2"
                                                               class="d-inline-flex border-radio_buttons" />Anual

                                                    </label>
                                                </div>

                                            </div>

                                        </div>

                                        <hr>
                                        <label for="">
                                            <h5 class="ms-1 color_letter">Descripción</h5>
                                        </label>
                                        <div class=" d-inline  ms-1 ">

                                            <textarea asp-for="Descripcion" name="descripcion" id="descripcion" class="size-text-area2"
                                                      placeholder="Describe tu propiedad mínimo con 250 caracteres"></textarea>
                                        </div>


                                    </div>
                                </div>

                                <!--aqui comienza la pestaña DIRECCION-->
                                <div class="tab-pane" id="direccion">
                                    <div class="row">
                                        <label for="direccion">
                                            <h5 class="color_letter">Dirección</h5>
                                        </label>
                                        <div class="input-group d-flex justify-content-between">
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group ">
                                                    <label for="c.p" class=" control-label color_letter2">
                                                        <h6>C.P.</h6>
                                                    </label>
                                                    <input type="number" asp-for="CP" name="CP" min="0" id="c.p" class="form-control NobtnNumber" />
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 size-nombre-calle ">
                                                <div class="d-inline form-group ">
                                                    <label for="n_calle" class=" control-label color_letter2">
                                                        <h6>Nombre de la calle</h6>
                                                    </label>
                                                    <input asp-for="Calle" name="Calle" type="text" id="n_calle"
                                                           class="form-control" />
                                                </div>
                                            </div>

                                            <div class="d-lg-inline-block ">
                                                <div class="d-inline form-group ">
                                                    <label for="n_interior" class=" control-label color_letter2">
                                                        <h6>No. interior</h6>
                                                    </label>
                                                    <input asp-for="NoInt" type="number" id="n_interior"
                                                           class="form-control NobtnNumber size-numeros-ext" />
                                                </div>
                                            </div>

                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group ">
                                                    <label for="n_exterior" class=" control-label color_letter2">
                                                        <h6>No. exterior</h6>
                                                    </label>
                                                    <input asp-for="NoExt" type="number" id="n_exterior"
                                                           class="form-control NobtnNumber size-numeros-ext" />
                                                </div>
                                            </div>




                                        </div>
                                        <div class="input-group d-flex justify-content-between mt-4">
                                            <div class="d-lg-inline-block me-3 size-nombre-calle">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="colonia" class=" control-label color_letter2">
                                                        <h6>Colonia</h6>
                                                    </label>
                                                    <input type="text" asp-for="Colonia" name="Colonia" id="colonia" class="form-control"
                                                           placeholder=" " />
                                                </div>
                                            </div>

                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="alc-muni" class=" control-label color_letter2">
                                                        <h6>Alcandia o Municipio</h6>
                                                    </label>
                                                    <input type="text" asp-for="Municipio" name="Municipio" id="alc-muni" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="estado" class=" control-label color_letter2">
                                                        <h6>Estado o Ciudad</h6>
                                                    </label>
                                                    <input type="text" asp-for="Estado" name="Estado" id="estado" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="pais" class=" control-label color_letter2">
                                                        <h6>País</h6>
                                                    </label>
                                                    <input type="text" asp-for="Pais" name="Pais" id="pais" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" asp-for="txtLat" name="txtLat" id="txtLat" class="form-control" placeholder="latitude"
                                                       style="visibility: hidden; overflow: hidden; display: none;">
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" asp-for="txtLng" name="txtLng" id="txtLng" class="form-control" placeholder="longitud"
                                                       style="visibility: hidden; overflow: hidden; display: none;">
                                            </div>
                                        </div>

                                        <div class="input-group mt-3">
                                            <label for="Direccion completa" class="color_letter2">
                                                <h6>Dirección completa</h6>
                                            </label>
                                            <div class="me-2">

                                                <input type="text" asp-for="DireccionC" name="DireccionC" id="direccion_completa" class="form-control me-2 w-50" readonly />
                                            </div>

                                        </div>


                                    </div>
                                    <div class="input-group mt-3">
                                        <div class="input-group mt-3" id="map" style="height:350px"></div>
                                    </div>

                                </div>

                                <!--aqui comienza la pestaña SUPERFICIE-->
                                <div class="tab-pane" id="superficie">
                                    <div class="row">

                                        <label for="superficie" class="fw-bold  ms-2">
                                            <h5 class="color_letter">Superficie</h5>
                                        </label>
                                        <br>
                                        <div class="input-group d-flex justify-content-between">

                                            <div class="me-1 mb-5 d-block">

                                                <label for="n_interior" class=" control-label color_letter2" style="margin-top: -8px !important;">
                                                    <h6>Terreno</h6>
                                                </label>

                                                <input type="text" asp-for="Superficie" name="Superficie" min="0" id="Superficieval"
                                                       class="form-control NobtnNumber  me-3" placeholder="" />
                                                @Html.DropDownListFor(model => model.SuperficieUMedida, new
                                                SelectList(Model.UMedidaL,"IDUm","UnidadMedida"), "-- Unidad Medida --",new
                                                {@class="size-select2 rounded text-center me-2", @id="UnidadSuperficie"})
                                            </div>

                                            <div class="d-block ">
                                                <label for="n_interior" class=" control-label color_letter2" style="margin-top: 0.6rem;">
                                                    <h6>Construcción</h6>
                                                </label>
                                                <input type="text" asp-for="SuperficieContruccion" name="SuperficieContruccion" min="0" id="SuperficieContruccion"
                                                       class="form-control NobtnNumber ms-2" />
                                                @Html.DropDownListFor(model => model.SuperficieContruccionUMedida, new
                                                SelectList(Model.UMedidaL,"IDUm","UnidadMedida"), "-- Unidad Medida --",new
                                                {@class="size-select2 rounded text-center ms-2",  @id="UnidadConstruccion"})
                                            </div>


                                            <div class=" d-block">
                                                <label for="n_interior" class=" control-label color_letter2">
                                                    <h6>Antigüedad (Años)</h6>
                                                </label>
                                                <input type="number" asp-for="Antiguedad" name="antiguedad" id="antiguedad" min="0"
                                                       class=" form-control me-3 NobtnNumber">

                                            </div>

                                            <div class="d-block borderadio">
                                                <label for="uso_de_suelo" class=" control-label color_letter2 ">
                                                    <h6>Uso de suelo</h6>
                                                </label>

                                                @Html.DropDownListFor(model => model.UsoSuelo, new
                                                SelectList(Model.SueloL,"IDSuelo","Suelo"), "-- Uso de suelo --",new
                                                {@class="size-select2 mt-suelo me-5 border rounded text-center top-select"})

                                            </div>

                                        </div>

                                    </div>
                                </div>

                                <!--aqui comienza CARACTERISTICAS-->
                                <div class="tab-pane" id="caracteristicas">

                                    <div class="row">
                                        <!---item 1-->
                                        <a class="collapsible2">Principales</a>
                                        <div class="content card-deck">

                                            <div class="row row-cols-3 mt-3 ms-4 ">
                                                <!--estimade desarrollador BACK-END: todas las cosas que pongas
                                                    en este div se acomodaran dinamicamente :)
                                                -->

                                                <div class=" d-inline-flex">
                                                    <label for="" class="color_letter2 align-items-center">Recámaras</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="Recamaras" type="number" min="0" class="form-control text-center ms-2 w-75 mt-1"
                                                               name="recamaras" id="recamarasval">

                                                    </div>
                                                </div>
                                                <div class="d-inline-flex">
                                                    <label for="" class=" color_letter2 align-items-center">Baños</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="Banos" type="number" min="0" class="form-control text-center ms-2 w-50 mt-1"
                                                               name="Banos" id="banosval">

                                                    </div>
                                                </div>
                                                <div class="d-inline-flex">
                                                    <label for="" class="color_letter2 align-items-center">Medios baños</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="MediosBanos" type="number" min="0" class="form-control text-center  ms-2 w-75 mt-1"
                                                               name="MediosBanos" id="">

                                                    </div>
                                                </div>
                                                <div class="d-inline-flex">
                                                    <label for="" class="color_letter2 align-items-center">Estacionamientos</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="Estacionamiento" type="number" min="0" class="form-control text-center ms-3 w-75 mt-1"
                                                               name="Estacionamiento" id="estacionamientoval">

                                                    </div>
                                                </div>

                                                <!--hasta aqui el dinamismo-->
                                            </div>


                                        </div>

                                        <!--item 2 -->
                                        <a class="collapsible2 mt-2 me-lg-3">Generales</a>
                                        <div class=" content card-deck">
                                            <div class="row row-cols-4 mt-3 ms-4">


                                                @for (var i = 0; i <= 10; ++i)
                                                {
                                                    <div class="d-inline-flex ms-4 me-4 mb-3 ">
                                                        @Html.CheckBoxFor(model => model.CaracteristicasL[i].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasL[i].IDCaracteristica})
                                                        @Html.HiddenFor(model => model.CaracteristicasL[i].IDCaracteristica)
                                                        <label for="" class="ms-2 color_letter2">
                                                            @Html.DisplayFor(model => model.CaracteristicasL[i].Caracteristica)
                                                        </label>

                                                    </div>
                                                }
                                                <!--termina acomodado dinamico-->
                                            </div>
                                        </div>

                                        <!--item 3-->
                                        <a class="collapsible2 mt-2  me-lg-3">Servicios</a>
                                        <div class="content card-deck">
                                            <div class="row row-cols-4 mt-3 ms-4">
                                                <!--estimade desarrollador BACK-END: todas las cosas que pongas
                                                    en este div se acomodaran dinamicamente :)
                                                -->
                                                @for (var i = 11; i <= 15; ++i)
                                                {
                                                    <div class="d-inline-flex ms-4 me-4 mb-3 ">
                                                        @Html.CheckBoxFor(model => model.CaracteristicasL[i].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasL[i].IDCaracteristica})
                                                        @Html.HiddenFor(model => model.CaracteristicasL[i].IDCaracteristica)
                                                        <label for="" class="ms-2 color_letter2">@Html.DisplayFor(model => model.CaracteristicasL[i].Caracteristica)</label>

                                                    </div>
                                                }


                                            </div>
                                        </div>


                                        <!--item 4 -->
                                        <a class="collapsible2 mt-2  me-lg-3">Exteriores</a>
                                        <div class="content card-deck">
                                            <div class="row row-cols-4 mt-3 ms-4">
                                                <!--estimade desarrollador BACK-END: todas las cosas que pongas
                                                    en este div se acomodaran dinamicamente :)
                                                -->
                                                @for (var i = 16; i <= 19; ++i)
                                                {
                                                    <div class="d-inline-flex ms-4 me-4 mb-4">
                                                        @Html.CheckBoxFor(model => model.CaracteristicasL[i].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasL[i].IDCaracteristica})
                                                        @Html.HiddenFor(model => model.CaracteristicasL[i].IDCaracteristica)
                                                        <label for="" class="ms-2 color_letter2">@Html.DisplayFor(model => model.CaracteristicasL[i].Caracteristica)</label>
                                                    </div>
                                                }


                                            </div>
                                        </div>

                                        <!--item 5-->
                                        <a class="collapsible2 mt-2 me-lg-3">Ambientes</a>
                                        <div class="content card-deck">
                                            <div class="row row-cols-4 mt-3 ms-4">
                                                <!--estimade desarrollador BACK-END: todas las cosas que pongas
                                                    en este div se acomodaran dinamicamente :)
                                                -->
                                                @for (var i = 20; i <= 21; ++i)
                                                {
                                                    <div class="d-inline-flex me-4 ms-4">
                                                        @Html.CheckBoxFor(model => model.CaracteristicasL[i].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasL[i].IDCaracteristica})
                                                        @Html.HiddenFor(model => model.CaracteristicasL[i].IDCaracteristica)
                                                        <label for="" class="ms-2 color_letter2">@Html.DisplayFor(model => model.CaracteristicasL[i].Caracteristica)</label>
                                                    </div>
                                                }

                                            </div>
                                        </div>

                                        <!--item 6-->
                                        <a class="collapsible2 mt-2 me-lg-3">Adicionales</a>
                                        <div class="content card-deck">
                                            <div class="row row-cols-3 mt-3 ms-4">
                                                <!--estimade desarrollador BACK-END: todas las cosas que pongas
                                                    en este div se acomodaran dinamicamente :)
                                                -->
                                                <div class="d-inline-flex  ">
                                                    <label for="" class="color_letter2 align-items-center">Bodegas</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="Bodegas" type="number" name="Bodegas" 
                                                        id="valBodegas" class="form-control w-50  mt-1" min="0">
                                                        <!--
                                                        <input  type="number"
                                                               class="form-control w-50  mt-1" min="0"><span class="material-input"></span>
                                                        -->
                                                    </div>
                                                </div>
                                                <div class="d-inline-flex ">
                                                    <label for="" class="color_letter2 align-items-center">Elevadores</label>
                                                    <div class="form-group is-empty">
                                                        <input type="number" asp-for="Elevadores" name="Elevadores" 
                                                        id="valElevadores" class="form-control w-75  mt-1" min="0">

                                                        <!--
                                                        <input type="number"
                                                               class="form-control w-75  mt-1" min="0"><span class="material-input"></span>
                                                        -->
                                                    </div>
                                                </div>
                                                <div class="d-inline-flex">
                                                    <label for="" class="align-items-center color_letter2">Closets</label>
                                                    <div class="form-group is-empty">
                                                        <input asp-for="Closets" name="Closets" id="valClosets" class="form-control ms-2 mt-1 w-50" min="0">
                                                        <!--
                                                        <input type="number" class="form-control ms-2 mt-1 w-50"
                                                               min="0"><span class="material-input"></span>
                                                        -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <!--comienza la pestaña DOCUMENTOS-->
                                <div class="tab-pane" id="documentos">
                                    <div class="row">
                                        <label for="documentos" class=" ms-lg-2">
