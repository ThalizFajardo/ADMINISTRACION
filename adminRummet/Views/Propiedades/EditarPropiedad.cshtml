@model PropiedadesModel;


@{
    Layout = "_NavUsuarios";
}


<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Proyecto_administracion</title>

    <link href="~/assets/css/material-bootstrap-wizard-2.css" rel="stylesheet">
    <link href="~/assets/css/material-bootstrap-wizard.css" rel="stylesheet">


    <!--<link rel="stylesheet" href="~/css/style_form_nuevo_usuario.css">
        -->
    <link rel="stylesheet" href="~/css/style_nueva_propiedad.css">
    <link rel="stylesheet" href="~/css/Style_form.css">

    <link rel="icon" type="image/png" sizes="16x16" href="~/img/imagen-logo/logo_oficial.png">
    <script src="https://kit.fontawesome.com/e43b4d35fb.js" crossorigin="anonymous"></script>
</head>


<div class="area">

    <ul class="circles">
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
    </ul>




    <!-- Progress bar -->
    <!--<div class="modal">
        <div class="modal-main">
          <div class="row">
            <div class="c-3-lg c-3-md c-1-sm close-modal"></div>
            <div class="c-6-lg c-6-md c-10-sm c-12-xs close-modal">
              <div class="modal-card" id="loading">
                <div class="preloader"></div>
                <span class="tag">Cargando...</span>
              </div>
              <div class="modal-card" id="Message">
                <span class="tag"></span>
              </div>
            </div>
            <div class="c-3-lg c-3-md c-1-sm close-modal"></div>
          </div>
        </div>
      </div>
    -->
    <!-- Progress bar -->

    <div class="container-lg ">
        <div class="row">

            <div class="col">
                <!--      Wizard container        -->
                
                    <div class="wizard-container">
                        
                        <div class="card wizard-card" data-color="orange" id="wizardProfile">

                          <form asp-controller="Propiedades" asp-action="GuardarEditarPropiedad" enctype="multipart/form-data" method="post">
                            <div class="wizard-navigation">
                                <ul>
                                    <li><a href="#generales" data-toggle="tab">Generales</a></li>
                                    <li><a href="#direccion" data-toggle="tab">Dirección</a></li>
                                    <li><a href="#superficie" data-toggle="tab">Superficie</a></li>
                                    <li><a href="#caracteristicas" data-toggle="tab">Características</a></li>
                                    <li><a href="#documentos" data-toggle="tab">Documentos</a></li>
                                    <li><a href="#propietario" data-toggle="tab">Propietario</a></li>
                                    <li><a href="#progr_agenda" data-toggle="tab">Programar agenda</a></li>
                                    <li><a href="#vista_previa" data-toggle="tab">Vista previa</a></li>
                                </ul>
                            </div>

                            <div class="tab-content">
                                <div class="tab-pane" id="generales">
                                    <div class="row">

                                        <!-- Steps -->
                                        <label for="">
                                            <h5 class="ms-1 color_letter">Datos generales</h5>
                                        </label>
                                        <div class="input-group justify-content-between  d-lg-inline-flex ">
                                            <div class="mt-3">
                                                <label class="color_letter2">
                                                    <h6>Tipo de operación</h6>
                                                </label>
                                                @if (Model.PropiedadEditar.Operacion == 1)
                                                {
                                                    <input asp-for="Operacion" type="radio" value="2" class="btn-check size-select " name="Operacion" id="Renta"
                                                       autocomplete="off" onclick="Mostrar()" />
                                                    <label class="btn btn-outline-primary rounded " for="Renta">Renta</label>

                                                    <input asp-for="Operacion" type="radio" value="1" class="btn-check" name="Operacion" id="Venta" autocomplete="off"
                                                       onclick="Ocultar()" checked />
                                                    <label class="btn btn-outline-primary rounded " for="Venta">Venta</label>
                                                }
                                                @if (Model.PropiedadEditar.Operacion == 2){
                                                    <input asp-for="Operacion" type="radio" value="2" class="btn-check size-select " name="Operacion" id="Renta"
                                                       autocomplete="off" checked onclick="Mostrar()" />
                                                    <label class="btn btn-outline-primary rounded " for="Renta">Renta</label>

                                                    <input asp-for="Operacion" type="radio" value="1" class="btn-check" name="Operacion" id="Venta" autocomplete="off"
                                                        onclick="Ocultar()"/>
                                                    <label class="btn btn-outline-primary rounded " for="Venta">Venta</label>
                                                }
                                            </div>
                                            <div class=" d-lg-inline-block  mb-3  ps-3 width-n-propiedad">
                                                <div class=" d-inline form-group label-floating  ">
                                                    <label for="n_propiedad" class=" color_letter2  control-label">
                                                        <h6>
                                                            Nombre de la
                                                            propiedad
                                                        </h6>
                                                    </label>
                                                    <input type="text" asp-for="NombrePropiedad" name="NombrePropiedad" id="nombre_propiedad"
                                                           class="form-control  w-100 margin-t-Npropiedad" value="@Model.PropiedadEditar.NombrePropiedad" required/>

                                                    <input type="text" asp-for="PropiedadGUID" name="PropiedadGUID" id="PropiedadID" class="form-control" placeholder="longitud"
                                                       style="visibility: hidden; overflow: hidden; display: none;" value=@Model.PropiedadGUID required>
                                                </div>
                                            </div>
                                            <div class=" d-lg-inline-block col-lg-4  ms-5 ">
                                                <label for="t_propiedad" class="text-start color_letter2  mt-2">
                                                    <h6>Tipo de propiedad</h6>
                                                </label>
                                                @Html.DropDownListFor(model => model.TPropiedad, new
                                                    SelectList(Model.TPropiedadL,"IDtipoP","Tipo"), "Tipo",new
                                                    {@class="size-select2 rounded text-center", @id="TipoPropiedadID"})

                                                @Html.DropDownListFor(model => model.SubPropiedad, new List<SelectListItem>(),
                                                "Subtipo", new { @class = "size-select2 rounded text-center d-lg-inline-block mt-md-2", @id="SubTipoPropiedadID" })

                                            </div>
                                        </div>
                                        <hr>
                                        <label for="" class="mt-lg-2">
                                            <h5 class="color_letter ms-1">Precio</h5>
                                        </label>



                                        <div class="input-group d-lg-inline-flex justify-content-between mt-lg-0 align-content-between ">
                                            <div class="d-lg-inline-block">
                                                <label for="" class=" text-start color_letter2   mt-lg-2">
                                                    <h6>Tipo de moneda</h6>
                                                </label>
                                                @Html.DropDownListFor(model => model.Moneda, new
                                                SelectList(Model.MonedaL,"IDMoneda","Moneda"), "Moneda",new
                                                {@class="size-select2 rounded text-center", @id="IDMoneda"})
                                            </div>
                                            <div class="d-lg-inline-block">
                                                <div class="d-inline form-group label-floating ">
                                                    <label for="precio" class=" control-label color_letter2">
                                                        <h6>Precio</h6>
                                                    </label>
                                                    <input type="number" asp-for="Precio" name="Precio" id="precioProp" class="form-control NobtnNumber"
                                                           min="0" value=@Model.PropiedadEditar.Precio required/>
                                                </div>
                                            </div>

                                            @*Se agrega una concion para poder mostrar los select*@
                                            @{ string radioPeriodo = "display:inline-block;";}
                                            @if( Model.PropiedadEditar.periodoRenta == null && Model.PropiedadEditar.periodoMantenimiento == null)
                                            {

                                                radioPeriodo = "display:none;";

                                            }

                                            <div id="renta" class="display-a_r me-lg-5 me-5 mt-lg-2 " style=@radioPeriodo>
                                                <label for="" class="me-3 color_letter2 ">
                                                    <h6 class="color_letter2">Periodo</h6>
                                                </label>

                                                @if( Model.PropiedadEditar.periodoRenta == 1){
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="1"
                                                            class="d-inline-flex border-radio_buttons" checked/>Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="2"
                                                            class="d-inline-flex border-radio_buttons"/>Anual
                                                    </label>
                                                } @if( Model.PropiedadEditar.periodoRenta == 2) {
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="1"
                                                            class="d-inline-flex border-radio_buttons" />Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="2"
                                                            class="d-inline-flex border-radio_buttons" checked/>Anual
                                                    </label>
                                                } @if( Model.PropiedadEditar.periodoRenta == null) {
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="1"
                                                            class="d-inline-flex border-radio_buttons" />Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoRenta" type="radio" name="PeriodoRenta" value="2"
                                                            class="d-inline-flex border-radio_buttons"/>Anual
                                                    </label>
                                                }
                                            </div>


                                            <div class="d-lg-inline-block me-5 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="n_propiedad" class=" control-label color_letter2">
                                                        <h6>Mantenimiento</h6>
                                                    </label>
                                                    <input asp-for="MantenimientoPrecio" type="number" name="MantenimientoPrecio" id="mantenimiento" 
                                                        class="form-control NobtnNumber" value=@Model.PropiedadEditar.Mantenimiento required/>
                                                </div>
                                            </div>

                                            <div id="renta2" class="display-a_r me-lg-5 me-5 mt-lg-2 " style=@radioPeriodo>
                                                <label for="" class="me-3 color_letter2 ">
                                                    <h6 class="color_letter2">Periodo</h6>
                                                </label>

                                                @if( Model.PropiedadEditar.periodoMantenimiento == 1){
                                                    <label>
                                                    <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="1"
                                                        class="d-inline-flex border-radio_buttons" checked/>Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="2"
                                                               class="d-inline-flex border-radio_buttons"/>Anual
                                                    </label>
                                                } @if( Model.PropiedadEditar.periodoMantenimiento == 2) {
                                                    <label>
                                                    <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="1"
                                                        class="d-inline-flex border-radio_buttons"/>Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="2"
                                                               class="d-inline-flex border-radio_buttons" checked/>Anual
                                                    </label>
                                                }
                                                @if (Model.PropiedadEditar.periodoMantenimiento == null)
                                                {
                                                    <label>
                                                    <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="1"
                                                        class="d-inline-flex border-radio_buttons"/>Mensual
                                                    </label>
                                                    <label>
                                                        <input asp-for="PeriodoMantenimiento" type="radio" name="PeriodoMantenimiento" value="2"
                                                               class="d-inline-flex border-radio_buttons"/>Anual
                                                    </label>
                                                }

                                            </div>
                                            </ hr>
                                        </div>
                                        <hr>
                                        <label for="">
                                            <h5 class="ms-1 color_letter">Descripción</h5>
                                        </label>
                                        <div class=" d-inline label-floating ms-1 ">

                                            <textarea name="descripcion" id="descripcion" class="size-text-area2" asp-for="Descripcion" required></textarea>

                                            @*<p>@Model.PropiedadEditar.Descripcion</p>*@

                                        </div>


                                    </div>
                                </div>


                                <div class="tab-pane" id="direccion">
                                    <div class="row">
                                        <label for="direccion">
                                            <h5 class="color_letter">Dirección</h5>
                                        </label>
                                        <div class="input-group d-flex justify-content-between">
                                            <div class="d-lg-inline-block me-3 size-nombre-calle">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="n_calle" class=" control-label color_letter2">
                                                        <h6>Nombre de la calle</h6>
                                                    </label>
                                                    <input type="text" asp-for="Calle" name="Calle" id="n_calle" class="form-control"
                                                           placeholder="" value=@Model.PropiedadEditar.Calle required/>
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="n_interior" class=" control-label color_letter2">
                                                        <h6>No. interior</h6>
                                                    </label>
                                                    <input type="number" asp-for="NoInt" name="NoInt" id="n_interior"
                                                           class="form-control size-numeros-ext NobtnNumber" value=@Model.PropiedadEditar.NoInt />
                                                </div>
                                            </div>

                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="n_exterior" class=" control-label color_letter2">
                                                        <h6>No. exterior</h6>
                                                    </label>
                                                    <input type="number" asp-for="NoExt" name="NoExt" id="n_exterior"
                                                           class="form-control size-numeros-ext NobtnNumber" value=@Model.PropiedadEditar.NoExt required/>
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="c.p" class=" control-label color_letter2">
                                                        <h6>C.P</h6>
                                                    </label>
                                                    <input type="number" asp-for="CP" name="CP" min="0" id="c.p" class="form-control NobtnNumber" value=@Model.PropiedadEditar.CP required/>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="input-group d-flex justify-content-between ">
                                            <div class="d-lg-inline-block me-3 size-nombre-calle">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="colonia" class=" control-label color_letter2">
                                                        <h6>Colonia</h6>
                                                    </label>
                                                    <input type="text" asp-for="Colonia" name="Colonia" id="colonia" class="form-control"
                                                           placeholder=" " value=@Model.PropiedadEditar.Colonia required/>
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="alc-muni" class=" control-label color_letter2">
                                                        <h6>Alcandia o Municipio</h6>
                                                    </label>
                                                    <input type="text" asp-for="Municipio" name="Municipio" id="alc-muni" class="form-control" value=@Model.PropiedadEditar.Municipio required/>
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="estado" class=" control-label color_letter2">
                                                        <h6>Estado o Ciudad</h6>
                                                    </label>
                                                    <input type="text" asp-for="Estado" name="Estado" id="estado" class="form-control" value=@Model.PropiedadEditar.Estado required/>
                                                </div>
                                            </div>
                                            <div class="d-lg-inline-block me-3 ">
                                                <div class="d-inline form-group label-floating">
                                                    <label for="pais" class=" control-label color_letter2">
                                                        <h6>País</h6>
                                                    </label>
                                                    <input type="text" asp-for="Pais" name="Pais" id="pais" class="form-control" value=@Model.PropiedadEditar.Pais required/>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" asp-for="txtLat" name="txtLat" id="txtLat" class="form-control" placeholder="latitude"
                                                       style="visibility: hidden; overflow: hidden; display: none;" value=@Model.PropiedadEditar.txtLat>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" asp-for="txtLng" name="txtLng" id="txtLng" class="form-control" placeholder="longitud"
                                                       style="visibility: hidden; overflow: hidden; display: none;" value=@Model.PropiedadEditar.txtLng>
                                            </div>
                                        </div>
                                        <div class="input-group mt-3">
                                            <label for="Direccion completa" class="color_letter2">
                                                <h6>Dirección completa</h6>
                                            </label>
                                            <div class="me-2">

                                                <input type="text" asp-for="DireccionC" name="DireccionC" id="direccion_completa" class="form-control me-2 w-50" value=@Model.PropiedadEditar.DireccionC readonly />
                                            </div>
                                        </div>
                                        <div class="input-group mt-3">
                                            <div class="input-group mt-3" id="map" style="height:350px"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane" id="superficie">
                                    <div class="row">

                                        <label for="superficie" class="fw-bold  ms-2">
                                            <h5 class="color_letter">Superficie</h5>
                                        </label>
                                        <br>
                                        <div class="input-group d-flex justify-content-between">

                                            <div class="me-1 mb-5 d-block">

                                                <label for="n_interior" class=" control-label color_letter2">
                                                    <h6>Terreno</h6>
                                                </label>
                                                <input type="number" asp-for="Superficie" name="Superficie" min="0" id="Superficieval"
                                                            class="form-control NobtnNumber  me-3" placeholder="" value=@Model.PropiedadEditar.Superficie/>
                                                @Html.DropDownListFor(model => model.SuperficieUMedida, new
                                                SelectList(Model.UMedidaL,"IDUm","UnidadMedida"), "-- Unidad Medida --",new
                                                {@class="size-select2 rounded text-center me-2", @id="UnidadSuperficie"})
                                            </div>



                                            <div class="d-block">
                                                <label for="n_interior" class=" control-label color_letter2">
                                                    <h6>Construcción</h6>
                                                </label>
                                                <input type="number" asp-for="SuperficieContruccion" name="SuperficieContruccion" min="0" id="SuperficieContruccion"
                                                    class="form-control NobtnNumber ms-2" value=@Model.PropiedadEditar.SuperficieContruccion/>
                                                @Html.DropDownListFor(model => model.SuperficieContruccionUMedida, new
                                                SelectList(Model.UMedidaL,"IDUm","UnidadMedida"), "-- Unidad Medida --",new
                                                {@class="size-select2 rounded text-center ms-2",  @id="UnidadConstruccion"})
                                            </div>




                                            <div class=" d-block">
                                                <label for="n_interior" class=" control-label color_letter2">
                                                    <h6>Antigüedad (Años)</h6>
                                                </label>
                                                <input type="number" asp-for="Antiguedad" name="antiguedad" id="antiguedad" min="0"
                                                       class=" form-control me-3 NobtnNumber" value=@Model.PropiedadEditar.Antiguedad>
                                            </div>
                                            <div class="d-block borderadio">
                                                <label for="uso_de_suelo" class=" control-label color_letter2 ">
                                                    <h6>Uso de suelo</h6>
                                                </label>
                                                 @Html.DropDownListFor(model => model.UsoSuelo, new
                                                  SelectList(Model.SueloL,"IDSuelo","Suelo"), "-- Uso de suelo --",new
                                                  {@class="size-select2 margin-t-residencial  me-5 border rounded text-center top-select", @id="UsoSuelo"})
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane" id="caracteristicas">
                                    <div class="row">
                                        <label for="caracteristicas" class="fw-bold">
                                            <h5 class="color_letter">Características</h5>
                                        </label>
                                        <a class="collapsible mb-2">Principales</a>
                                        <div class="content">
                                            <div class="input-group d-flex justify-content-between me-lg-2 ms-lg-4">
                                                <div class="form-floating d-flex">
                                                    <label for="" class="color_letter2">Recámaras</label>
                                                    <input asp-for="Recamaras" type="number" min="0" class="form-control text-center ms-2 w-75 mt-1"
                                                        name="recamaras" id="recamarasval" value=@Model.CaracteristicasNumPropiedadEditar[0].numeroElementos required > @*valor para las recamaras*@

                                                        
                                                </div>
                                                <div class="form-floating d-flex">
                                                    <label for="" class="label-floating color_letter2">Baños</label>
                                                   <input asp-for="Banos" type="number" min="0" class="form-control text-center ms-2 w-50 mt-1" 
                                                        name="Banos" id="banosval" value=@Model.CaracteristicasNumPropiedadEditar[2].numeroElementos required > @*valor para los baños*@
                                                </div>
                                                <div class="form-floating d-flex">
                                                    <label for="" class="label-floating color_letter2">Medios baños</label>
                                                    <input asp-for="MediosBanos" type="number" min="0" class="form-control text-center  ms-2 w-75 mt-1" 
                                                        name="MediosBanos" id="" value=@Model.CaracteristicasNumPropiedadEditar[3].numeroElementos required > @*valor para los Medios Baños*@
                                                </div>
                                                <div class="form-floating d-flex me-2">
                                                    <label for="" class="label-floating color_letter2">Estacionamientos</label>
                                                    <input asp-for="Estacionamiento" type="number" min="0" class="form-control text-center ms-3 w-75 mt-1" 
                                                        name="Estacionamiento" id="estacionamientoval" value=@Model.CaracteristicasNumPropiedadEditar[1].numeroElementos required > @*valor para los estacionamientos*@
                                                </div>
                                            </div>
                                        </div>
                                        <br>
                                        <a class="collapsible mb-2 mt-2 me-2">Generales</a>
                                        <div class="content">
                                            <div class="input-group d-flex  me-lg-2 ms-lg-3">
                                                <div class="row">
                                                    <div class="col mt-3">
                                                        <div class="col justify-content-between d-flex">
                                                            <div class="d-inline-flex me-3 ms-2">
                                                                @Html.CheckBoxFor(model => model.CaracteristicasCheckPropiedadEditar[0].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasCheckPropiedadEditar[0].IDCaracteristica})
                                                                @Html.HiddenFor(model => model.CaracteristicasCheckPropiedadEditar[0].IDCaracteristica)
                                                                <label for="" class="ms-1 color_letter2">@Html.DisplayFor(model => model.CaracteristicasCheckPropiedadEditar[0].Caracteristica)</label>
                                                            </div>
                                                            <div class="d-inline-flex me-5">
                                                                @Html.CheckBoxFor(model => model.CaracteristicasCheckPropiedadEditar[1].isSelected, new {@class = "size-adicionales ms-1", @id=@Model.CaracteristicasCheckPropiedadEditar[1].IDCaracteristica})
                                                                @Html.HiddenFor(model => model.CaracteristicasCheckPropiedadEditar[1].IDCaracteristica)
                                                                <label for="" class="ms-1 color_letter2">@Html.DisplayFor(model => model.CaracteristicasCheckPropiedadEditar[1].Caracteristica)</label>
                                                            </div>
                                                            <div class="d-inline-flex me-4">
                                                                @Html.CheckBoxFor(model => model.CaracteristicasCheckPropiedadEditar[2].isSelected, new {@class = "size-adicionales", @id=@Model.CaracteristicasCheckPropiedadEditar[2].IDCaracteristica})
                                                                @Html.HiddenFor(model => model.CaracteristicasCheckPropiedadEditar[2].IDCaracteristica)
                                                                <label for="" class="ms-1 color_letter2">@Html.DisplayFor(model => model.CaracteristicasCheckPropiedadEditar[2].Caracteristica)</label>
